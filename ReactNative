//Install React-Native

npx create-expo-app [ProjectName] --template blank 

npm start

// you can get apk without expo web app on your android folder 
1. npm install -g expo-cli    // this command install expo-cli 
2. npx expo prebuild --platform android  // this command create android and iso folder in expo 
3. cd android ./gradlew assembleRelease  // this command build apk if no error found  

eas login
eas build --platform android 
// to build abb file on expo web app if you want direct apk you can get command for apk 

to convert from .abb file to apk
java -jar bundletool-all-1.18.1.jar build-apks ^
  --bundle="application-018aafb6-28d6-41d3-8fe3-f4fe7cba0fe2.aab" ^
  --output="my-multi-charts-app.apks" ^
  --mode=universal ^
  --ks="@hanzla0001__multi_charts-keystore.bak.jks" ^
  --ks-pass=pass:YOUR_KEYSTORE_PASSWORD ^
  --ks-key-alias=YOUR_KEY_ALIAS ^
  --key-pass=pass:YOUR_KEY_PASSWORD

